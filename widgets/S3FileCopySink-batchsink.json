{
  "metadata": {
    "spec-version": "1.3"
  },
  "configuration-groups": [
    {
      "label": "S3 File Copy Sink",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        },
        {
          "widget-type": "textbox",
          "label": "URI",
          "name": "filesystemURI"
        },
        {
          "widget-type": "textbox",
          "label": "Base Path",
          "name": "basePath"
        },
        {
          "widget-type": "select",
          "label": "Enable Overwrite",
          "name": "enableOverwrite",
          "widget-attributes": {
            "values": [
              "true",
              "false"
            ],
            "default": "false"
          }
        },
        {
          "widget-type": "select",
          "label": "Preserve File Owner",
          "name": "preserveFileOwner",
          "widget-attributes": {
            "values": [
              "true",
              "false"
            ],
            "default": "false"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Buffer Size (MB)",
          "name": "bufferSize",
          "default": "1"
        },
        {
          "widget-type": "textbox",
          "label": "Access Key ID",
          "name": "accessKeyId"
        },
        {
          "widget-type": "password",
          "label": "Secret Key ID",
          "name": "secretKeyId"
        }
      ]
    }
  ]
}
